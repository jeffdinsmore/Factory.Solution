@{
  Layout = "_Layout";
}

<br>
<div class="jumbotron">
  <h2>Welcome To Dr. Sillystringz's Factory Database</h2>
</div>
<div class="col-md-5">
  <br>
  <ul>
    <h3>List of Engineers:</h3>
    @foreach (var engineer in ViewBag.Eng)
    {
      <li>@engineer.EngineerName</li>
    }
    <br>
    <p>@Html.ActionLink("Go to Engineers", "Index", "Engineers", null, new {@class="btn btn-primary btn-sm"})</p>
  </ul>
</div>
<br>
<div class="col-md-5">
  <ul>
    <h3>List of Machines:</h3>
    @foreach (var machine in ViewBag.Mach)
    {
      <li>@machine.MachineName</li>
    }
    <br>
    <p>@Html.ActionLink("Go to Machines", "Index", "Machines", null, new {@class="btn btn-primary btn-sm"})</p>
  </ul>
</div>